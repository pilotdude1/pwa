{
	"name": "svelteship-template",
	"version": "0.0.1",
	"private": true,
	"engines": {
		"node": "18.19.0",
		"npm": "10.2.4"
	},
	"scripts": {
		"dev": "vite dev",
		"clean": "rm -rf .svelte-kit build node_modules/.vite",
		"prepare": "svelte-kit sync",
		"prebuild": "npm run clean",
		"build": "vite build --mode production && node scripts/postbuild.js",
		"start": "node build/index.js",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"test_run": "vitest run",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"format_check": "prettier --check --plugin prettier-plugin-svelte ./src",
		"postinstall": "svelte-kit sync",
		"generate-icons": "node scripts/generate-icons.js"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-node": "^5.2.12",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tailwindcss/typography": "^0.5.10",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.16",
		"daisyui": "^4.0.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.0",
		"postcss": "^8.4.31",
		"prettier": "^3.1.0",
		"prettier-plugin-svelte": "^3.1.0",
		"stripe": "^14.0.0",
		"svelte": "^4.2.0",
		"svelte-check": "^3.6.0",
		"svelte-loading-spinners": "^0.3.6",
		"tailwindcss": "^3.4.0",
		"theme-change": "^2.0.2",
		"typescript": "^5.3.0",
		"vite": "^5.0.0",
		"vite-plugin-pwa": "^1.0.0",
		"vitest": "^1.0.0",
		"workbox-precaching": "^7.3.0",
		"workbox-routing": "^7.3.0",
		"workbox-strategies": "^7.3.0"
	},
	"type": "module",
	"dependencies": {
		"@stripe/stripe-js": "^2.0.0",
		"@supabase/auth-ui-shared": "^0.1.8",
		"@supabase/auth-ui-svelte": "^0.2.9",
		"@supabase/ssr": "^0.1.0",
		"@supabase/supabase-js": "^2.49.7",
		"puppeteer": "^24.9.0",
		"sass-embedded": "^1.69.0",
		"workbox-window": "^7.3.0"
	}
}
